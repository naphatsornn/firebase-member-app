{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "x-access-token",
          "value": ":x-access-token"
        },
        {
          "key": "tmn-access-token",
          "value": ":tmn-access-token"
        },
        {
          "key": "authorization",
          "value": ":authorization"
        }
      ]
    }
  ]
}
